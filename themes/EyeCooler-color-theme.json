{
  "name": "EyeCooler",
  "colors": {
    "editor.background": "#000000",
    "editor.foreground": "#eeffff",

    // Menu Bar and Title Bar
    "menubar.selectionBackground": "#000000", // Background when menu item is selected (hovered/clicked)
    "menubar.selectionForeground": "#FFFFFF", // Text color of selected item
    "titleBar.activeBackground": "#000000", // Background of the whole top title bar (when focused)
    "titleBar.activeForeground": "#FFFFFF", // Text/icon color of title bar (when focused)
    "titleBar.inactiveBackground": "#000000", // Title bar when window is unfocused
    "titleBar.inactiveForeground": "#AAAAAA", // Text/icon color when unfocused
    "titleBar.border": "#ff7b00",

    // Selection
    "focusBorder": "#ff7b00", // Transparent border when focused
    "widget.border": "#ff7b00", // Transparent border around widgets (dropdowns, find box, etc.)
    "list.focusOutline": "#ff7b00", // List item (like files or extensions) outline on focus
    "tree.indentGuidesStroke": "#ff7b00", // Optional: removes tree line strokes

    // Button
    "button.background": "#ff7b00",
    "button.foreground": "#000000",
    "button.hoverBackground": "#fd912c",
    "button.secondaryBackground": "#ff7b00",
    "button.secondaryForeground": "#000000",
    "button.secondaryHoverBackground": "#ff7b00",
    "button.secondaryHoverForeground": "#000000",

    // Input
    "input.background": "#000000",
    "input.foreground": "#eeffff",
    "input.placeholderForeground": "#f3d2b3",
    "input.border": "#ff7b00",
    "inputOption.activeBackground": "#ff7b00",
    "inputOption.activeBorder": "#ff7b00",
    "inputValidation.errorBackground": "#ff002b",
    "inputValidation.errorBorder": "#ff002b",
    "inputValidation.infoBackground": "#ff7b00",
    "inputValidation.infoBorder": "#ff7b00",
    "inputValidation.warningBackground": "#ff7b00",
    "inputValidation.warningBorder": "#ff7b00",

    // Editor
    "editorCursor.foreground": "#ffffff",
    "editor.lineHighlightBackground": "#000000",
    "editor.lineHighlightBorder": "#ff7b00",
    "editor.selectionBackground": "#795129",
    "editor.selectionForeground": "#000000",
    "editor.selectionHighlightBackground": "#ff7b00",
    "editor.selectionHighlightBorder": "#ff7b00",
    "editor.wordHighlightBackground": "#795129",
    "editor.wordHighlightBorder": "#ff7b00",
    "editor.findMatchBackground": "#ff7b00",
    "editor.findMatchHighlightBackground": "#ff7b00",
    "editor.findRangeHighlightBackground": "#ff7b00",
    "editor.findMatchBorder": "#ff7b00",
    "editor.findMatchHighlightBorder": "#ff7b00",
    "editor.findRangeHighlightBorder": "#ff7b00",
    "editor.hoverHighlightBackground": "#795129",
    // "editor.hoverHighlightBorder": "#ff7b00",
    "editor.wordHighlightStrongBackground": "#ff7b00",
    "editor.wordHighlightStrongBorder": "#ff7b00",
    "editorBracketMatch.background": "#ff7b00",
    "editorBracketMatch.border": "#ff7b00",
    "editorBracketHighlight.foreground1": "#ff7b00",
    "editorBracketHighlight.foreground2": "#ff7b00",
    "editorBracketHighlight.foreground3": "#ff7b00",
    "editorBracketHighlight.foreground4": "#ff7b00",
    "editorBracketHighlight.foreground5": "#ff7b00",
    "editorBracketHighlight.foreground6": "#ff7b00",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ff7b00",
    "editorError.foreground": "#ff002b",
    "editorWarning.foreground": "#ff7b00",
    "editorInfo.foreground": "#ff7b00",
    "editorGutter.background": "#000000",
    "editorGutter.commentRangeForeground": "#ff7b00",
    "editorGutter.insertedBackground": "#ff7b00",
    "editorGutter.deletedBackground": "#ff002b",
    "editorGutter.modifiedBackground": "#ff7b00",
    "editorGutter.addedBackground": "#ff7b00",

    // Status Bar
    "statusBar.background": "#ff7b00", // background color of status bar (default blue)
    "statusBar.foreground": "#000000", // text color in status bar
    "statusBar.noFolderBackground": "#68217A", // color when no folder is open
    "statusBar.debuggingBackground": "#CC6633", // color while debugging
    "statusBarItem.remoteBackground": "#693709", // Background of remote connection item
    "statusBarItem.remoteForeground": "#ffffff", // Text/icon color in remote section

    // List
    "list.activeSelectionBackground": "#ff7b00", // Background of selected item (active window)
    "list.activeSelectionForeground": "#FFFFFF", // Text color of selected item (active window)
    "list.inactiveSelectionBackground": "#373737", // Background when inactive (not focused)
    "list.inactiveSelectionForeground": "#CCCCCC", // Text color when inactive
    "list.hoverBackground": "#2A2D2E", // Background when you hover over an item
    "list.hoverForeground": "#FFFFFF", // Text color on hover
    // "list.border"

    // Terminal
    "terminal.background": "#000000", // Background color
    "terminal.foreground": "#FFFFFF", // Default text color
    "terminalCursor.background": "#FFFFFF", // Cursor background (rarely needed)
    "terminalCursor.foreground": "#FFFFFF", // Cursor color
    // "terminal.selectionBackground": "#555555", // Selection highlight color
    "terminal.selectionBackground": "#ff7b00",
    "terminal.inactiveSelectionBackground": "#000000",
    "terminal.border": "#ff7b00",
    "panel.border": "#ff7b00",
    "panelTitle.activeBorder": "#ff7b00",
    // "panelTitle.activeForeground": "#ff7b00",

    // Terminal ANSI Colors (output colors)
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#FF5F56",
    "terminal.ansiGreen": "#27C93F",
    "terminal.ansiYellow": "#FFBD2E",
    "terminal.ansiBlue": "#1E90FF",
    "terminal.ansiMagenta": "#FF00FF",
    "terminal.ansiCyan": "#00FFFF",
    "terminal.ansiWhite": "#FFFFFF",
    "terminal.ansiBrightBlack": "#555555",
    "terminal.ansiBrightRed": "#FF6E67",
    "terminal.ansiBrightGreen": "#5FFA68",
    "terminal.ansiBrightYellow": "#FFF75E",
    "terminal.ansiBrightBlue": "#6A9FFF",
    "terminal.ansiBrightMagenta": "#FF9FFF",
    "terminal.ansiBrightCyan": "#9FFFFF",
    "terminal.ansiBrightWhite": "#FFFFFF",

    // Activity Bar
    "activityBar.background": "#000000", // Background color
    "activityBar.foreground": "#FFFFFF", // Icon and text color
    "activityBar.inactiveForeground": "#888888", // Inactive icons
    "activityBar.activeBorder": "#ff7b00", // Border under active icon
    "activityBar.border": "#ff7b00", // Border around activity bar
    "activityBarBadge.background": "#ff7b00", // Background of badges (e.g., update count)
    "activityBarBadge.foreground": "#000000", // Text color inside badge

    // Side Bar
    "sideBar.background": "#000000", // Whole sidebar background (includes Extensions)
    "sideBar.foreground": "#CCCCCC", // Text color in sidebar
    "sideBarTitle.foreground": "#FFFFFF", // Title text (e.g., "EXTENSIONS")
    "sideBarSectionHeader.background": "#ff7b00", // Section header (e.g., "INSTALLED")
    "sideBarSectionHeader.foreground": "#000000",
    "sideBarSectionHeader.border": "#ff7b00",
    "scrollbarSlider.background": "#55555588",
    "scrollbarSlider.hoverBackground": "#777777AA",
    "sideBar.border": "#ff7b00",
    "icon.foreground": "#000000",

    // Links
    "textLink.foreground": "#ff7b00", // Normal link color
    "textLink.activeForeground": "#664322", // On hover/click
    "textPreformat.foreground": "#CCCCCC", // Inline code in markdown
    "textBlockQuote.background": "#1E1E1E", // Background of blockquotes
    "textBlockQuote.border": "#ff7b00" // Left border of blockquotes
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#02aefd"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#1ae6e6"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#500e0e"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ff002b"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9440cc"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#ac71f0"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#336ce9"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#df6881"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#46bee2"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
